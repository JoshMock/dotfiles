#!/usr/bin/env python3

from __future__ import print_function

import sys
from html import unescape

try:
    input_stream = sys.stdin.buffer
except AttributeError:
    input_stream = sys.stdin

custom_replace = [
    ("&dquot;", '"'),
    ("&open;", "["),
    ("&close;", "]"),
]

# strip html entities
task = input_stream.readline().decode("utf-8", errors="replace")

for before, after in custom_replace:
    task = task.replace(before, after)

task = unescape(task)

print(task)
